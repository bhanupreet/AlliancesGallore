<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alliancesgalore.alliancesgalore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name="com.alliancesgalore.alliancesgalore.RegisterActivity"
            android:parentActivityName="com.alliancesgalore.alliancesgalore.StartActivity"></activity>
        <activity android:name="com.alliancesgalore.alliancesgalore.StartActivity" />
        <activity android:name="com.alliancesgalore.alliancesgalore.LoginActivity"
            android:parentActivityName="com.alliancesgalore.alliancesgalore.StartActivity"/>
        <activity android:name="com.alliancesgalore.alliancesgalore.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name="com.alliancesgalore.alliancesgalore.MainActivity" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>